.header {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  z-index: 1000;
  transition: 0.3s;
  padding: 2% 6% 2% 5%;
}

.header .logo {
  font-size: 25px;
  font-weight: 800;
  text-decoration: underline;
  color: #001c30;
}

section {
  padding: 2.5em 8em 5em 10em;
}

.home {
  position: relative;
  min-height: 100vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.home .content {
  width: 85%;
  margin-left: -6%;
  margin-top: 50px;
  z-index: 900;
}

.home .content h1 {
  color: #d9edf7;
  font-size: 50px;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 2px;
  line-height: 55px;
  margin-bottom: 15px;
  font-weight: bolder;
}

.home .content h1 span {
  font-size: 40px;
  font-weight: 600;
  line-height: 35px !important;
}

.home .content p {
  margin-bottom: 50px;
}

.home .content a {
  color: #d9edf7;
  text-decoration: none;
  padding: 15px 35px;
  border-radius: 2px;
  font-size: 18px;
  font-weight: 500;
  border-radius: 15px;
}

.home .icons {
  position: absolute;
  right: 7%;
  top: 45%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  z-index: 900;
}

.home .icons a {
  font-size: 20px;
  margin-bottom: 20px;
  transition: 0.3s;
}

.home .icons a:hover {
  transform: scale(1.3);
}

.home video {
  z-index: 0;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  object-fit: cover;
  clip-path: circle(0% at 0 50%);
}

.home video.active {
  clip-path: circle(150% at 0 50%);
  transition: 2s;
}

.home::before {
  position: fixed;
  content: "";
  width: 100%;
  height: 100%;
  background: rgba(1, 1, 1, 0.3);
  z-index: 700;
  left: 0;
  top: 0;
}

.video-nav {
  z-index: 800;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20px;
  transform: translateY(80px);
  margin-bottom: 12px;
}

.video-nav .nav-item {
  width: 15px;
  height: 15px;
  background: #fff;
  border-radius: 50px;
  cursor: pointer;
  transition: 0.3s;
}

.video-nav .nav-item.active {
  width: 30px;
}

/* **********CATEGORYIES******** */

.categories {
  background: #001c30;
}

.categories h1 {
  line-height: 1;
  font-size: 60px;
}

.container_categories {
  display: grid;
}

.categories_left {
  display: flex;
  align-items: center;
  flex-direction: column;
  text-align: center;
  color: #001c30;
}

.categories_left p {
  margin: 1rem 0 3rem;
  color: #001c30;
}

.categories_right {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 1.2rem;
  cursor: pointer;
}

.category {
  background: #002b36;
  padding: 2rem;
  border-radius: 2rem;
  transition: var(--transition);
  font-weight: 800;
}

.category h5 {
  color: #ccc;
}

.categories_right i {
  color: #faf9f6;
}

.category:nth-child(2) .category-icon {
  background: var(--color-danger);
}

.category:nth-child(3) .category-icon {
  background: var(--color-succes);
}

.category:nth-child(4) .category-icon {
  background: var(--color-warning);
}

.category:nth-child(5) .category-icon {
  background: var(--color-succes);
}

.category:nth-child(6) .category-icon {
  background: var(--color-danger);
}

.category-icon {
  background: var(--color-primary);
  padding: 0.7rem;
  border-radius: 0.9rem;
  color: #1b2925;
}

.category h5 {
  margin: 2rem 0 1rem;
  color: #1b2925;
}

.category p {
  font-size: 0.85rem;
  color: #1b2925;
}

@media (max-width: 1049px) {
  .header {
    padding: 2% 6% 2% 5%;
  }

  section {
    padding: 6em 2em 6em 4em;
  }

  .home .icons {
    right: 8%;
  }

  .drop-container {
    display: none;
  }
}

@media (max-width: 990px) {
  .header .nav {
    position: fixed;
    height: 100vh;
    width: 100%;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: rgba(1, 1, 1, 0.5);
    display: none;
  }

  .header.active .nav {
    display: flex;
  }

  .header .nav a {
    background: #fff;
    color: #000;
    width: 500px;
    padding: 20px;
    text-align: center;
    font-size: 19px;
    margin-left: 0px;
  }

  .header .nav a:hover {
    background: #222;
    color: #fff;
  }

  .header .menu-btn {
    width: 40px;
    height: 40px;
    background: url(media/menu.png) no-repeat;
    background-size: 30px;
    background-position: center;
    filter: invert(1);
    cursor: pointer;
    transition: 0.3s;
